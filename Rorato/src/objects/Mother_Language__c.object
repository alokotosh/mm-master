    </nameField>
    <pluralLabel>Mother Languages</pluralLabel>
    <searchLayouts/>
    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>Language_Code</fullName>
        <active>true</active>
        <description>Validate the possible language code from Mother language object to allow only uppercase letters and the code maximum length should be 3</description>
        <errorConditionFormula>OR(Possible_language_code__c   &lt;&gt; UPPER(Possible_language_code__c), LEN(Possible_language_code__c)&gt;3 )</errorConditionFormula>
        <errorDisplayField>Possible_language_code__c</errorDisplayField>
        <errorMessage>Possible language code allow only uppercase letters and the code maximum length should be 3</errorMessage>
    </validationRules>
